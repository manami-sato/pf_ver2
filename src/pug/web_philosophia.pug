						dd pug／Sass／JavaScript／jQuery／JSON
					div.link
						a(href="http://click.ecc.ac.jp/ecc/msatou/Philosophia/",target="site")
							span ウェブサイトへ
						a(href="#",target="github")
							span GitHubへ
				div.conceptsheet
					img(src="img/conceptsheet_02_philosophia.png")
			a(href="works_web.html",target="page",class="arrow")
				span back
		include _foot.pug